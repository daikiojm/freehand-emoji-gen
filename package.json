{
  "name": "freehand-emoji-gen",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "yarn run current-git-sha nuxt",
    "build": "yarn run current-git-sha nuxt build",
    "predev": "yarn run copy:gifjs",
    "prebuild": "yarn run copy:gifjs",
    "pregenerate": "yarn run copy:gifjs",
    "start": "yarn run current-git-sha nuxt start",
    "generate": "yarn run current-git-sha nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "upgrade:interactive": "yarn upgrade-interactive --latest",
    "copy:gifjs": "cp ./node_modules/gif.js/dist/gif.worker.js* ./static/js/",
    "current-git-sha": "NUXT_ENV_CURRENT_GIT_SHA=`git rev-parse --short HEAD`"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/composition-api": "^0.24.0",
    "@pixi/filter-drop-shadow": "^4.1.3",
    "@pixi/filter-glitch": "^4.1.3",
    "@pixi/filter-pixelate": "^4.1.3",
    "@pixi/filter-zoom-blur": "^4.1.3",
    "@vueuse/core": "^4.11.1",
    "core-js": "^3.12.1",
    "gif.js": "^0.2.0",
    "nuxt": "^2.15.5",
    "nuxt-i18n": "^6.27.0",
    "perfect-freehand": "^0.4.71",
    "pixi.js": "^6.0.4",
    "rxjs": "^7.1.0",
    "tinycolor2": "^1.4.2",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@nuxt/types": "^2.15.5",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.3",
    "@types/gif.js": "^0.2.0",
    "@types/jest": "^27.0.1",
    "@types/tinycolor2": "^1.4.2",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^11.1.2",
    "postcss": "^7",
    "prettier": "^2.3.2",
    "sass": "^1.34.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4",
    "vue-jest": "^3.0.4"
  }
}
